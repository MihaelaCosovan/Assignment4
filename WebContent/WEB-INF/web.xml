<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>C5WebApp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
 
 <servlet> <servlet-name> WelcomePage </servlet-name> <servlet-class>ServletContainer.WelcomePage</servlet-class> </servlet> 
 <servlet-mapping> <servlet-name> WelcomePage </servlet-name> <url-pattern> /welcome </url-pattern> </servlet-mapping>
 
  <servlet> <servlet-name> SelectOption </servlet-name> <servlet-class>ServletContainer.SelectOption</servlet-class> </servlet> 
  <servlet-mapping> <servlet-name> SelectOption </servlet-name> <url-pattern> /select </url-pattern> </servlet-mapping>
 
  <servlet> <servlet-name> InsertCredentials </servlet-name> <servlet-class>ServletContainer.InsertCredentials</servlet-class> </servlet> 
  <servlet-mapping> <servlet-name> InsertCredentials </servlet-name> <url-pattern> /login </url-pattern> </servlet-mapping>
 
   <servlet> <servlet-name> LoginStatus </servlet-name> <servlet-class>ServletContainer.LoginStatus</servlet-class> </servlet> 
   <servlet-mapping> <servlet-name> LoginStatus </servlet-name> <url-pattern> /loginStatus </url-pattern> </servlet-mapping>
   
      <servlet> <servlet-name> RegisterUser </servlet-name> <servlet-class>ServletContainer.RegisterUser</servlet-class> </servlet> 
   <servlet-mapping> <servlet-name> RegisterUser </servlet-name> <url-pattern> /register </url-pattern> </servlet-mapping>
   
   <servlet> <servlet-name> InsertUser </servlet-name> <servlet-class>ServletContainer.InsertUser</servlet-class> </servlet> 
   <servlet-mapping> <servlet-name> InsertUser </servlet-name> <url-pattern> /insert </url-pattern> </servlet-mapping>
   
      <servlet> <servlet-name> QuerryDB </servlet-name> <servlet-class>ServletContainer.QuerryDB</servlet-class> </servlet> 
   <servlet-mapping> <servlet-name> QuerryDB </servlet-name> <url-pattern> /querryDB </url-pattern> </servlet-mapping>
   
</web-app>